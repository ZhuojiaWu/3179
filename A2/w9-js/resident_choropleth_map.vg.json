{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "Number of Residents return to Australia 2019-2023",
    "width": 800,
    "height": 400,
    "projection": {"type": "equalEarth"},
    "data": {
      "url": "https://raw.githubusercontent.com/ZhuojiaWu/3179/main/A2/w9-js/output.json",
      "format": {"type": "topojson", "feature": "STE_2021_AUST_GDA2020"}
    },
    "transform": [
      {
        "lookup": "properties.STE_NAME21",
        "from": {
          "data": {
            "url": "https://raw.githubusercontent.com/ZhuojiaWu/3179/main/A2/Data/Short-term%20resident%20returns%2C%20state%2019-23.csv" 
          },
          "key": "Stateorterritory",
          "fields": ["Jul2019","Jul2021","Jul2022","Jul2023"]
        }
      },
        { "calculate": "datum.Jul2023+datum.Jul2022+datum.Jul2021+datum.Jul2019",
           "as": "Total"
        }
        
      
    ],
    "mark": {"type": "geoshape"},
    "encoding": {
      "color": {
        "field": "Total",
        "type": "quantitative",
        "scale": {"type": "log"},
        "title": "Number of residents return"
      },
      "tooltip": [
        {"field": "properties.STE_NAME21", "type": "nominal", "title": "states"},
        {"field": "Total", "type": "quantitative"}
      ]
    }
  }