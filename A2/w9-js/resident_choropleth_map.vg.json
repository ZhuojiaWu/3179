{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "Number of Residents return to Australia 2019-2023",
    "width": 800,
    "height": 400,
    "projection": {"type": "equalEarth"},
    "data": {
      "url": "https://github.com/ZhuojiaWu/3179/blob/main/A2/w9-js/output.json",
      "format": {"type": "topojson", "feature": "STE_2021_AUST_GDA2020"}
    },
    "transform": [
      {
        "lookup": "properties.STE_NAME21",
        "from": {
          "data": {
            "url": "https://github.com/ZhuojiaWu/3179/blob/main/A2/Data/Short-term%20resident%20returns%2C%20state%2019-23.csv" 
          },
          "key": "State or territory"
        }
      },
        {
          "calculate": "datum['Jul 2019'] + datum['Jul 2021'] + datum['Jul 2022'] + datum['Jul 2023']",
          "as": "Total"
        }
      
    ],
    "mark": {"type": "geoshape"},
    "encoding": {
      "color": {
        "field": "Total",
        "type": "quantitative",
        "scale": {"type": "log"}
      },
      "tooltip": [
        {"field": "properties.STE_NAME21", "type": "nominal", "title": "states"},
        {"field": "Total", "type": "quantitative"}
      ]
    }
  }
